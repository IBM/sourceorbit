-- Project Master
CREATE OR REPLACE TABLE PRJMST (
--  SQL150B   10   REUSEDLT(*NO) in table PRJMST in PAYROLL1 ignored.
        ACREC CHAR(1) CCSID 37 NOT NULL DEFAULT '' ,
        PRCDE CHAR(8) CCSID 37 NOT NULL DEFAULT '' ,
        PRDSC CHAR(50) CCSID 37 NOT NULL DEFAULT '' ,
        PRRSP CHAR(30) CCSID 37 NOT NULL DEFAULT '' ,
        PRSTR DECIMAL(6, 0) NOT NULL DEFAULT 0 ,
        PREND DECIMAL(6, 0) NOT NULL DEFAULT 0 ,
        PRCMP DECIMAL(6, 0) NOT NULL DEFAULT 0 ,
        PREST DECIMAL(9, 1) NOT NULL DEFAULT 0 ,
        PRHRC DECIMAL(7, 1) NOT NULL DEFAULT 0 ,
        PRHRY DECIMAL(9, 1) NOT NULL DEFAULT 0 ,
        PRHRP DECIMAL(9, 1) NOT NULL DEFAULT 0 ,
        PRIMARY KEY( PRCDE ) )

        RCDFMT RCPRJ      ;

LABEL ON COLUMN PRJMST
( PRCDE IS 'PROJECT             CODE' ,
        PRDSC IS 'PROJECT             DESCRIPTION' ,
        PRRSP IS 'PROJECT             RESPONSIBILITY' ,
        PRSTR IS 'PRJ START           DATE' ,
        PREND IS 'PRJ EST             END DATE' ,
        PRCMP IS 'PRJ CMP             DATE' ,
        PREST IS 'PRJ EST             TOT HRS' ,
        PRHRC IS 'PRJ HRS             CUR MTH' ,
        PRHRY IS 'PRJ HRS             YTD' ,
        PRHRP IS 'PRJ HRS             PRIOR YR' ) ;

LABEL ON COLUMN PRJMST
( ACREC TEXT IS 'ACTIVE RECORD CODE' ,
        PRCDE TEXT IS 'PROJECT CODE' ,
        PRDSC TEXT IS 'PROJECT DESCRIPTION' ,
        PRRSP TEXT IS 'PROJECT RESPONSIBILITY' ,
        PRSTR TEXT IS 'PROJECT START DATE' ,
        PREND TEXT IS 'PROJECT ESTIMATED END DATE' ,
        PRCMP TEXT IS 'PROJECT COMPLETION DATE' ,
        PREST TEXT IS 'PROJECT ESTIMATED TOTAL HRS' ,
        PRHRC TEXT IS 'PROJECT HOURS CURRENT MONTH' ,
        PRHRY TEXT IS 'PROJECT HOURS YEAR TO DATE' ,
        PRHRP TEXT IS 'PROJECT HOURS PRIOR YEAR' ) ;

GRANT DELETE , INSERT , SELECT , UPDATE
ON PRJMST TO PUBLIC ;

